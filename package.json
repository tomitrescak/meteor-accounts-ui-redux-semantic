{
  "name": "apollo-authentication-semantic-ui",
  "version": "1.0.10",
  "description": "UI for apollo authentication",
  "main": "./dist/index.js",
  "scripts": {
    "test": "mocha --require ./mocha.js --ui snapshots --report lcovonly 'src/**/*.test.ts*' --compilers ts:ts-node/register",
    "testjs": "mocha --require ./mocha.js --ui snapshots --report lcovonly 'dist/**/*.test.js'",
    "test_fuse": "node fuse test",
    "coverage": "NODE_ENV=test nyc npm run test && nyc report --reporter=lcov",
    "luis": "node fuse luis",
    "updateSnapshots": "UPDATE_SNAPSHOTS=true npm run test"
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://tomitrescak@github.com/tomitrescak/apollo-authentication-semantic-ui.git"
  },
  "keywords": [
    "Apollo",
    "Authentication"
  ],
  "author": "Tomi Trescak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomitrescak/apollo-authentication-semantic-ui/issues"
  },
  "homepage": "https://github.com/tomitrescak/apollo-authentication-semantic-ui#readme",
  "peerDependencies": {},
  "devDependencies": {
    "@types/graphql": "^0.10.1",
    "@types/react": "^15.0.39",
    "apollo-client": "^1.9.0-1",
    "apollo-mantra": "^1.6.1",
    "apollo-module-authentication": "1.2.0",
    "apollo-modules": "^1.0.7",
    "apollo-test-utils": "^0.3.2",
    "es2015-i18n-tag": "^1.1.6",
    "fuse-box": "2.2.2-beta.6",
    "fuse-test-runner": "https://github.com/tomitrescak/fuse-test-runner",
    "graphql-tools": "^1.1.0",
    "luis": "^1.2.49",
    "mobx-react": "^4.2.2",
    "mocha": "^3.4.2",
    "nyc": "^11.1.0",
    "pre-commit": "^1.2.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "semantic-ui-mobx": "^1.1.8",
    "sweetalert2": "^6.6.6",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.2",
    "typestyle": "^1.3.7",
    "wafl": "^0.2.2"
  },
  "pre-commit": [
    "test"
  ],
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
